{
  "name": "docktor",
  "description": "Docker Monitoring",
  "version": "0.0.1",
  "private": true,
  "contributors": [
    "Yvonnick Esnault - yesnault",
    "Fran√ßois Samin - fsamin",
    "Gwendal Leclerc - gwleclerc",
    "Mathieu Cornic - macornic",
    "Thibaut Rousseau - Thiht"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/soprasteria/docktor.git"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x",
    "go": "1.7"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./client/dist ./docktor",
    "client:start": "webpack-dev-server",
    "client:build": "NODE_ENV=production webpack",
    "client:test": "mocha --require babel-core/register --require babel-polyfill client/test/browser.js client/test/**/*.tests.js",
    "client:lint": "eslint '*.jsx?' 'client/src/**/*.jsx?' 'client/test/**/*.jsx?' && sass-lint 'client/**/*.s?css' ",
    "client:fix": "eslint --fix '*.jsx?' 'client/src/**/*.jsx?' 'client/test/**/*.jsx?'",
    "server:start": "make run",
    "server:build": "make build",
    "server:test": "make test",
    "server:lint": "make lint",
    "start": "npm run clean && npm run client:start & npm run server:start",
    "build": "npm run clean && npm run client:build && npm run server:build",
    "test": "npm run client:test && npm run server:test",
    "prepackage": "npm run build && mkdir -p dist/client && mv client/dist dist/client && mv docktor dist",
    "package": "cd dist && tar zcvf ../docktor-${npm_package_version}.tar.gz docktor client && cd .."
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^3.5.0",
    "css-loader": "^0.28.0",
    "dateformat": "^2.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^6.10.3",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.28.0",
    "jsdom": "^10.0.0",
    "mocha": "^3.3.0",
    "node-sass": "^4.5.2",
    "optimize-css-assets-webpack-plugin": "^1.3.1",
    "prop-types": "^15.5.8",
    "run-sequence": "^1.2.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.3",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4",
    "write-file-webpack-plugin": "^4.0.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "isomorphic-fetch": "^2.2.1",
    "joi-browser": "^10.0.6",
    "leaflet": "^1.0.3",
    "lodash.differenceby": "^4.8.0",
    "lodash.flatmap": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "react": "^15.5.4",
    "react-custom-scrollbars": "^4.1.1",
    "react-debounce-input": "^2.4.2",
    "react-dom": "^15.5.4",
    "react-leaflet": "^1.1.6",
    "react-notification-system": "^0.2.13",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "rodal": "1.4.0",
    "semantic-ui-css": "^2.2.10",
    "semantic-ui-react": "^0.68.2",
    "uuid-js": "^0.7.5"
  }
}
