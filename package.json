{
  "name": "docktor",
  "description": "Docker Monitoring",
  "version": "0.0.1",
  "private": true,
  "contributors": [
    "Yvonnick Esnault - yesnault",
    "Fran√ßois Samin - fsamin",
    "Gwendal Leclerc - gwleclerc",
    "Mathieu Cornic - macornic",
    "Thibaut Rousseau - Thiht"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/soprasteria/docktor.git"
  },
  "engines": {
    "node": "0.10.x",
    "npm": "1.4.x",
    "go": "1.7"
  },
  "scripts": {
    "clean": "rm -rf ./dist ./client/dist ./docktor",
    "client:start": "webpack-dev-server",
    "client:build": "NODE_ENV=production webpack",
    "client:test": "mocha --require babel-core/register --require babel-polyfill client/test/browser.js client/test/**/*.tests.js",
    "client:lint": "eslint '*.js?(x)' 'client/src/**/*.js?(x)' 'client/test/**/*.js?(x)' && sass-lint 'client/**/*.scss' ",
    "client:fix": "eslint --fix '*.js?(x)' 'client/src/**/*.js?(x)' 'client/test/**/*.js?(x)'",
    "server:start": "make run",
    "server:build": "make build",
    "server:test": "make test",
    "server:lint": "make lint",
    "start": "npm run clean && npm run client:start & npm run server:start",
    "build": "npm run clean && npm run client:build && npm run server:build",
    "test": "npm run client:test && npm run server:test",
    "prepackage": "mkdir -p dist/client && mv client/dist dist/client && mv docktor dist",
    "package": "cd dist && tar zcvf ../docktor-${npm_package_version}.tar.gz docktor client && cd .."
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.7",
    "dateformat": "^2.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.6.1",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-react": "^7.3.0",
    "estraverse-fb": "^1.3.2",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "prop-types": "^15.5.10",
    "redux-devtools-extension": "^2.13.2",
    "run-sequence": "^2.1.0",
    "sass-lint": "^1.11.1",
    "sass-loader": "^6.0.6",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.7.1"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "joi-browser": "^10.6.1",
    "leaflet": "^1.2.0",
    "lodash.deburr": "^4.1.0",
    "lodash.differenceby": "^4.8.0",
    "lodash.flatmap": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.groupby": "^4.6.0",
    "lodash.set": "^4.3.2",
    "lodash.sortby": "^4.7.0",
    "lodash.uniqby": "^4.7.0",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "normalizr": "^3.2.3",
    "query-string": "^5.0.0",
    "react": "^15.6.1",
    "react-custom-scrollbars": "^4.1.2",
    "react-debounce-input": "^3.0.1",
    "react-dom": "^15.6.1",
    "react-leaflet": "^1.6.5",
    "react-notification-system": "^0.2.15",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.72.0",
    "uuid-js": "^0.7.5"
  }
}
